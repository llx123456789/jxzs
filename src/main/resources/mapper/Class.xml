<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pwj.jxzs.dao.ClassDao">
    <insert id="insert">
        insert into class(class_id,class_name,class_kemu,class_xueqi,class_area,class_school,class_number,user_id)
        values(#{classid},#{classname},#{classkemu},#{classxueqi},#{classarea},#{classschool},#{classnumber},#{usersid})
    </insert>

</mapper>